        shortLabel Comprehensive
        longLabel Comprehensive Gene Annotation Set from ENCODE/GENCODE Version M7 (Ensembl 82)
        type genePred
        priority 2
    
        track wgEncodeGencodePseudoGeneVM7
        trackHandler wgEncodeGencode
        subTrack wgEncodeGencodeVM7ViewGenes off
        subGroups view=aGenes name=Pseudogenes 
        shortLabel Pseudogenes
        longLabel Pseudogene Annotation Set from ENCODE/GENCODE Version M7 (Ensembl 82)
        type genePred
        color 255,51,255
        priority 3
    
    track wgEncodeGencodeVM7View2Way 
    shortLabel 2-Way
    view b2-way
    visibility hide
    subTrack wgEncodeGencodeVM7
    type genePred
    configurable off

        track wgEncodeGencode2wayConsPseudoVM7
        subTrack wgEncodeGencodeVM7View2Way off
        subGroups view=b2-way name=yTwo-way
        shortLabel 2-way Pseudogenes
        longLabel 2-way Pseudogene Annotation Set from ENCODE/GENCODE Version M7 (Ensembl 82)
        type genePred
        color 255,51,255
        priority 4
    
    track wgEncodeGencodeVM7ViewPolya
    shortLabel PolyA
    view cPolya
    visibility hide
    subTrack wgEncodeGencodeVM7
    type genePred
    configurable off

        track wgEncodeGencodePolyaVM7
        trackHandler wgEncodeGencode
        subTrack wgEncodeGencodeVM7ViewPolya off
        subGroups view=cPolya name=zPolyA
        shortLabel PolyA
        longLabel PolyA Transcript Annotation Set from ENCODE/GENCODE Version M7 (Ensembl 82)
        type genePred
        color 0,0,0
        priority 5
    
