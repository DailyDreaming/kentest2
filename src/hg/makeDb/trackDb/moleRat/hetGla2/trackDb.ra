# Local declaration so that local gold.html is picked up.
track gold override
html gold

# Local declaration so that local gap.html is picked up.
track gap override
html gap

track chainNetMm10 override
visibility pack

track gc5BaseBw override
visibility full

track genscan override
visibility pack

track xenoMrna override
visibility dense

searchTable gold
searchMethod prefix
searchType bed
shortCircuit 1
termRegex [AN][CH][K_][G0][0-9]+(\.1)?
query select chrom,chromStart,chromEnd,frag from %s where frag like '%s%%'
searchPriority 8

track ensGene override
visibility pack
ensemblIdUrl http://www.ensembl.org/Heterocephalus_glaber_female

include ../../refSeqComposite.ra
