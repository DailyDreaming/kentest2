################################################################
# sudEbo1 - Sudan ebolavirus
################################################################
track chainNetSudEbo1
release alpha
compositeTrack on
shortLabel $o_Organism Chain
longLabel $o_Organism ($o_date), Chain Alignments
subGroup1 view Views chain=Chain net=Net
dragAndDrop subTracks
visibility hide
group compGeno
noInherit on
priority 100
color 0,0,0
altColor 255,255,0
type bed 3
sortOrder view=+
matrix 16 91,-90,-25,-100,-90,100,-100,-25,-25,-100,100,-90,-100,-25,-90,91
matrixHeader A, C, G, T
otherDb sudEbo1
chainMinScore 100
chainLinearGap loose
spectrum on
html chainNet

    track chainNetSudEbo1Viewchain
    release alpha
    shortLabel Chain
    view chain
    visibility pack
    parent chainNetSudEbo1
    spectrum on

        track chainSudEbo1
        release alpha
        parent chainNetSudEbo1Viewchain
        subGroups view=chain
        shortLabel $o_Organism Chain
        longLabel $o_Organism ($o_date) Chained Alignments
        type chain sudEbo1
        html chainNet

